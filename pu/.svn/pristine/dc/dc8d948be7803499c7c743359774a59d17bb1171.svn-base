package nc.vo.pu.m23.env;

import java.io.Serializable;

import nc.vo.pub.lang.UFBoolean;

/**
 * @since 6.0
 * @version 2011-3-17 下午07:30:44
 * @author yinfy
 */

public class ArrivalUIToBSEnv implements Serializable {
  private static final long serialVersionUID = 7955562531482060916L;

  /** 是否确认退货 */
  private UFBoolean bBack = UFBoolean.FALSE;

  /** 用户选择的结果：是或者否 */
  private UFBoolean bConfirm = UFBoolean.FALSE;

  /** 某个动作（脚本）是否由前台触发 **/
  private UFBoolean bManual = UFBoolean.FALSE;

  public UFBoolean getbBack() {
    return this.bBack;
  }

  public UFBoolean getbConfirm() {
    return this.bConfirm;
  }

  public UFBoolean getbManual() {
    return this.bManual;
  }

  public void setbBack(UFBoolean bBack) {
    this.bBack = bBack;
  }

  public void setbConfirm(UFBoolean bConfirm) {
    this.bConfirm = bConfirm;
  }

  public void setbManual(UFBoolean bManual) {
    this.bManual = bManual;
  }
}
