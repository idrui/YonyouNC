package nc.ui.pu.pub.action;

import nc.ui.pubapp.uif2app.actions.LinkQueryAction;
import nc.ui.scmpub.action.SCMActionInitializer;
import nc.vo.scmpub.res.SCMActionCode;

@SuppressWarnings("restriction")
public class PULinkQueryAction extends LinkQueryAction {

  private static final long serialVersionUID = 8987920019025406757L;

  public PULinkQueryAction() {
    super();
    SCMActionInitializer
        .initializeAction(this, SCMActionCode.SCM_BILLLINKQUERY);
    this.setOpenMode(1);
  }
}
