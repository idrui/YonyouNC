package nc.vo.pu.report.view.order;

import nc.vo.pub.JavaType;

/**
 * @since 6.0
 * @version 2011-9-15 обнГ04:43:04
 * @author wuxla
 */

public class RptExtAttribute {
  private JavaType javaType;

  private String name;

  private int type;

  public RptExtAttribute(String name, int type, JavaType javaType) {
    this.name = name;
    this.type = type;
    this.javaType = javaType;
  }

  public JavaType getJavaType() {
    return this.javaType;
  }

  public String getName() {
    return this.name;
  }

  public int getType() {
    return this.type;
  }
}
