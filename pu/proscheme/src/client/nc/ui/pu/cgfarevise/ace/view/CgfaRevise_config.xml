						</bean>
					</property>
				</bean>
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardHeadTailAfterEditEvent" />
					<property name="handler">
						<bean class="nc.ui.pu.cgfa.ace.event.CardHeadTailAfterEditEventHandler">
							<property name="billForm" ref="billForm" />
						</bean>
					</property>
				</bean>
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardBodyAfterEditEvent" />
					<property name="handler">
						<bean class="nc.ui.pu.cgfa.ace.event.CardBodyAfterEditEventHandler">
							<property name="billForm" ref="billForm" />
						</bean>
					</property>
				</bean>
				<!-- modify end -->


			</list>
		</property>
	</bean>
